<section class="card shadow-sm border-0">
  <div class="card-body">
    <h1 class="h4 mb-3">{{title}}</h1>
    <div het-component="acquisition-duplicate-seed">
      <p het-props="textContent=count:seed[int]" class="mb-2">1</p>
      <p het-props="textContent=count:seed[int]" class="mb-0">2</p>
    </div>
    <p id="error-message" class="mb-0 mt-3 text-muted">No error</p>
  </div>
</section>
<script nonce="{{{cspNonce}}}">
  window.HET.registerComponent('acquisition-duplicate-seed', {
    setup: () => ({}),
  });
  window.addEventListener('load', () => {
    const message = window.hetErrors.at(-1);
    if (message) {
      document.getElementById('error-message').textContent = message;
    }
  });
</script>
