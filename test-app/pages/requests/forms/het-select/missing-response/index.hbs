<section het-pane="main" class="card shadow-sm border-0">
  <div class="card-body">
    <h1 class="h4 mb-3">{{title}}</h1>
    <p id="primary" class="text-muted">Primary content.</p>
    <p id="missing-response">Current-only content.</p>
    <form id="form" method="get" action="/requests/forms/het-select/responses/missing" het-target="main" het-select="missing-response">
      <button id="submit" type="submit" class="btn btn-outline-secondary">Update missing response</button>
    </form>
  </div>
</section>
