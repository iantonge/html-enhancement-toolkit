<section het-pane="main" class="card shadow-sm border-0">
  <div class="card-body">
    <h1 class="h4 mb-3">{{title}}</h1>
    <p id="primary" class="text-muted">Primary content.</p>
    <p id="secondary">Secondary content.</p>
    <p id="tertiary">Tertiary content.</p>
    <a
      id="multi-link"
      href="/requests/links/het-select/responses/partial"
      het-target="main"
      het-select="primary secondary"
    >
      Update primary + secondary
    </a>
  </div>
</section>
