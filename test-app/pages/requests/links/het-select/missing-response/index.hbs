<section het-pane="main" class="card shadow-sm border-0">
  <div class="card-body">
    <h1 class="h4 mb-3">{{title}}</h1>
    <p id="primary" class="text-muted">Primary content.</p>
    <p id="missing-response">Current-only content.</p>
    <a
      id="missing-response-link"
      href="/requests/links/het-select/responses/missing"
      het-target="main"
      het-select="missing-response"
    >
      Update missing response
    </a>
  </div>
</section>
