<section id="main-pane" het-pane="main" class="card shadow-sm border-0">
  <div class="card-body">
    <h1 class="h4 mb-3">{{title}}</h1>
    <p id="main-content" class="text-muted">UI feedback content.</p>
    <button id="inline-button" class="btn btn-outline-secondary" type="button">Inline</button>
  </div>
</section>
<aside class="mt-3 d-flex gap-2 flex-wrap">
  <a id="slow-link" class="btn btn-outline-secondary" href="/requests/links/ui-feedback/responses/slow" het-target="main" het-select="main-content">
    Slow request
  </a>
  <a id="slow-link-2" class="btn btn-outline-secondary" href="/requests/links/ui-feedback/responses/slow-2" het-target="main" het-select="main-content">
    Slow request 2
  </a>
  <a id="custom-class-link" class="btn btn-outline-secondary" href="/requests/links/ui-feedback/custom-class">
    Custom class scenario
  </a>
</aside>
