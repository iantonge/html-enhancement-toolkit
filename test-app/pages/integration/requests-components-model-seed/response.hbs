<section id="main-pane" het-pane="main" class="card shadow-sm border-0">
  <div class="card-body">
    <h1 id="seed-title" class="h4 mb-3">{{title}}</h1>
    <div id="integration-model-seed" het-component="integration-model-seed">
      <input id="seed-input" class="form-control" het-model="query:seed" value="beta">
      <p id="seed-value" het-props="textContent=query" class="mt-2 text-muted mb-0"></p>
    </div>
  </div>
</section>
